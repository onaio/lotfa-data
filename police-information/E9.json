{
    "label": "Summary of Political Violence and Protest: Violence against civilians",
    "source": {
        "type": "vector",
        "data": "https://raw.githubusercontent.com/onaio/lotfa-data/master/police-information/data/af-acled9.csv",
        "layer": "af-acled-timeseries-5ktmt2",
        "url": "mapbox://ona.2e8163gd",
        "join": [
            "data_id",
            "data_id"
        ]
    },
    "type": "circle",
    "property": "fatalities",
    "radius-range": [
        "9",
        "12",
        "15"
    ],
    "categories": {
        "breaks": "no",
        "color": ["#EC6C09","#EC6C09","#EC6C09"],
        "limit": [
            20,
            70,
            100000
        ],
        "label": [
            "<20",
            ">20 < 70",
            "above 70"
        ]
      
    },
    "aggregate": {
        "timeseries": {
            "field": "period"
        }
    },
    "popup": {
        "header": "",
