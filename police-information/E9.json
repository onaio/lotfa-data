    },
    "visible": false,
    "credit": "",
    "category": "Boundaries",
    "charts": {
        "pie": {
            "type": "breaks",
            "level": "event_type",
            "column": "fatalities"
        },
        "primary": {
            "type": "column",
            "spec": {
                "type": "percent",
                "level": "event_type",
                "column": "fatalities"
            }
        }
    }
